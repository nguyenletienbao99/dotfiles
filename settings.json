{
  "editor.formatOnSave": true,
  "editor.fontSize": 15,
  "editor.fontWeight": "400",
  "terminal.integrated.fontFamily": "FuraCode Nerd Font",
  "editor.fontFamily": "FuraCode Nerd Font",
  "terminal.integrated.fontSize": 15,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.renderLineHighlight": "none",
  "editor.occurrencesHighlight": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "window.restoreWindows": "folders",
  "editor.suggestSelection": "first",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*.class": true,
    "**/*.form": true,
    "**/*.min.css": true,
    "node_modules/": true,
    "yarn.lock": true,
    "build": true,
    ".next/": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "node_modules/": true,
    "**/bower_components": true,
    "**/tmp": true
  },
  "editor.largeFileOptimizations": false,
  "explorer.confirmDelete": false,
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "material-icon-theme",
  "extensions.ignoreRecommendations": true,
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "terminal.integrated.allowChords": false,
  "security.workspace.trust.untrustedFiles": "open",
  "json.schemas": [],
  "errorLens.enabledDiagnosticLevels": ["warning", "info"],
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.editorAssociations": {
    "*.jar": "default"
  },
  "eslint.format.enable": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "redhat.telemetry.enabled": false,
  "[typescript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[go]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "golang.go"
  },
  "bracketPairColorizer.depreciation-notice": false,
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "workbench.colorTheme": "GitHub Dark",
  "go.toolsManagement.autoUpdate": true,
  "background.fullscreen": {
    "images": ["https://i.ibb.co/7YjMRNg/zoro.jpg"],
    "opacity": 0.97,
    "size": "cover",
    "position": "center",
    "interval": 0
  },
  "workbench.colorCustomizations": {
    "statusBar.background": "#8FBCBB",
    "statusBar.noFolderBackground": "#8FBCBB",
    "statusBar.debuggingBackground": "#8FBCBB",
    "statusBar.foreground": "#434C5E",
    "statusBar.debuggingForeground": "#434C5E"
  },
  "tabnine.experimentalAutoImports": true,
  "liveSassCompile.settings.formats": [
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": null,
      "savePathReplacementPairs": null
    }
  ],
  "liveSassCompile.settings.useNewCompiler": true,
  "liveSassCompile.settings.generateMap": false,
  "files.associations": {
    "*.md": "ftl"
  },
  "launch": {
    "configurations": [],
    "compounds": []
  }
}
